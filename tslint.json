{
    "extends": "tslint:recommended",
    "rules": {
        "member-access": false,
        "trailing-comma": [
            true,
          {
            "multiline": "never",
            "singleline": "never"
          }
        ],
        "max-line-length": [
            false
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case",
            "allow-leading-underscore"
        ],
        "arrow-return-shorthand": [
            true
        ],
        "unified-signatures": true,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            "tabs"
        ]
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ],
    "jsRules": {
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "tabs"
        ],
        "no-duplicate-variable": true,
        "no-unused-variable": [
            true
        ],
        "no-eval": true,
        "no-trailing-whitespace": true,
        "no-unsafe-finally": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}