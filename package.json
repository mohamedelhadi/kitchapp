  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve --browser %npm_package_config_browser%",
    "preionic:serve": "npm run initialize",
    "_preinitialize": "npm run tslint",
    "initialize": "babel-node src/_build/cli",
    "finalize": "babel-node src/_build/cli --action copy-output",
    "android-dev": "npm run initialize dev && ionic run android && npm run finalize dev",
    "android-testing": "npm run initialize testing && ionic run android && npm run finalize testing",
    "android-staging": "npm run initialize staging && ionic run android && npm run finalize staging",
    "android-production": "npm run initialize production && ionic run android --release --prod --generateSourceMap && npm run finalize production",
    "ios-dev": "%npm_package_cli% dev ios && ionic run ios",
    "ios-testing": "%npm_package_cli% testing ios && ionic run ios",
    "ios-staging": "%npm_package_cli% staging ios && ionic run ios",
    "ios-prod": "%npm_package_cli% production ios && ionic run ios",
    "tslint": "tslint \"./src/**/*.ts\"  \"./src/**/*.js\" --color"
  },
  "dependencies": {
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/compiler-cli": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/animations": "4.1.0",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
